<?xml version="1.0"?>
<configuration>
	<multihreadingMode>false</multihreadingMode>
	<updateOntologyAndMappingsPeriod>10</updateOntologyAndMappingsPeriod>
	<language name ="en">
		<extractors>
			<extractor name ="org.dbpedia.extraction.mappings.LabelExtractor" status="PURGE">
				<produces>
					<matchPattern type="EXACT">
						<s></s>
						<p>RDFS_LABEL</p>
						<o></o>
					</matchPattern>
				</produces>
			</extractor>

			<extractor name ="org.dbpedia.extraction.mappings.WikiPageExtractor" status="PURGE">
				<produces>
					<matchPattern type="STARTSWITH" pexact ="true">
						<s></s>
						<p>FOAF_PAGE</p>
						<o>http://en.wikipedia.org/wiki/</o>
					</matchPattern>
					<matchPattern type="STARTSWITH" pexact ="true">
						<s></s>
						<p>DB_WIKIPAGE_EN</p>
						<o>http://en.wikipedia.org/wiki/</o>
					</matchPattern>
				</produces>
				<!--Must be set in source code-->
				<notice>set in source code</notice>
			</extractor>

			<extractor name ="org.dbpedia.extraction.mappings.InfoboxExtractor" status="PURGE">
				<produces>
					<matchPattern type="STARTSWITH">
						<s></s>
						<p>DB_PROPERTY_NS</p>
						<o></o>
					</matchPattern>
				</produces>
				<notice>I deactivated warning output for catchObjectDataTypeFunctions, this was for a test run only</notice>
			</extractor>

			<extractor name ="org.dbpedia.extraction.mappings.PageLinksExtractor" status="PURGE">
				<produces>
					<matchPattern type="EXACT">
						<s></s>
						<p>DB_WIKILINK</p>
						<o></o>
					</matchPattern>
				</produces>
			</extractor>

			<extractor name ="org.dbpedia.extraction.mappings.GeoExtractor" status="PURGE">
				<produces>
					<matchPattern type="EXACT">
						<s></s>
						<p>GEORSS_POINT</p>
						<o></o>
					</matchPattern>
					<matchPattern type="EXACT">
						<s></s>
						<p>WGS_LAT</p>
						<o></o>
					</matchPattern>
					<matchPattern type="EXACT">
						<s></s>
						<p>WGS_LONG</p>
						<o></o>
					</matchPattern>
					<matchPattern type="EXACT">
						<s></s>
						<p>GEO_FEATURECLASS</p>
						<o></o>
					</matchPattern>
					<matchPattern type="EXACT">
						<s></s>
						<p>RDF_TYPE</p>
						<o>YAGO_LANDMARK</o>
					</matchPattern>
					<matchPattern type="EXACT">
						<s></s>
						<p>GEO_POPULATION</p>
						<o></o>
					</matchPattern>
					<matchPattern type="EXACT">
						<s></s>
						<p>GEORSS_RADIUS</p>
						<o></o>
					</matchPattern>
					<matchPattern type="EXACT">
						<s></s>
						<p>GEO_FEATURECODE</p>
						<o></o>
					</matchPattern>
				</produces>
				<postprocessing type="IFEXISTSDONOTDELETE">in dbpedia gecoordinates might exist in another language version, but is not produced by the english live extraction</postprocessing>
			</extractor>

			<extractor name ="org.dbpedia.extraction.mappings.ExternalLinksExtractor" status="PURGE">
				<produces>
					<matchPattern type="EXACT">
						<s></s>
						<p>DB_REFERENCE</p>
						<o></o>
					</matchPattern>
				</produces>
			</extractor>

			<extractor name ="org.dbpedia.extraction.mappings.HomepageExtractor" status="PURGE">
				<produces>
					<matchPattern type="EXACT">
						<s></s>
						<p>FOAF_HOMEPAGE</p>
						<o></o>
					</matchPattern>
				</produces>
			</extractor>

			<extractor name ="org.dbpedia.extraction.mappings.DisambiguationExtractor" status="PURGE">
				<produces>
					<matchPattern type="EXACT">
						<s></s>
						<p>DB_DISAMBIGUATES</p>
						<o></o>
					</matchPattern>
				</produces>
			</extractor>

			<extractor name ="org.dbpedia.extraction.mappings.PersondataExtractor" status="PURGE">
				<produces>
					<matchPattern type="EXACT">
						<s></s>
						<p>FOAF_NAME</p>
						<o></o>
					</matchPattern>
					<matchPattern type="EXACT">
						<s></s>
						<p>FOAF_GIVENNAME</p>
						<o></o>
					</matchPattern>
					<matchPattern type="EXACT">
						<s></s>
						<p>FOAF_SURNAME</p>
						<o></o>
					</matchPattern>
					<matchPattern type="EXACT">
						<s></s>
						<p>DC_DESCRIPTION</p>
						<o></o>
					</matchPattern>
					<matchPattern type="EXACT">
						<s></s>
						<p>RDF_TYPE</p>
						<o>FOAF_PERSON</o>
					</matchPattern>
					<matchPattern type="EXACT">
						<s></s>
						<p>DB_BIRTH</p>
						<o></o>
					</matchPattern>
					<matchPattern type="EXACT">
						<s></s>
						<p>DB_BIRTHPLACE</p>
						<o></o>
					</matchPattern>
					<matchPattern type="EXACT">
						<s></s>
						<p>DB_DEATH</p>
						<o></o>
					</matchPattern>
					<matchPattern type="EXACT">
						<s></s>
						<p>DB_DEATHPLACE</p>
						<o></o>
					</matchPattern>
				</produces>
			</extractor>

			<extractor name ="org.dbpedia.extraction.mappings.PndExtractor" status="PURGE">
				<produces>
					<matchPattern type="EXACT">
						<s></s>
						<p>DBO_INDIVIDUALISED_PND</p>
						<o></o>
					</matchPattern>
					<matchPattern type="EXACT">
						<s></s>
						<p>DBO_NON_INDIVIDUALISED_PND</p>
						<o></o>
					</matchPattern>
				</produces>
			</extractor>

			<extractor name ="org.dbpedia.extraction.mappings.SkosCategoriesExtractor" status="PURGE">
				<produces>
					<matchPattern type="EXACT">
						<s></s>
						<p>SKOS_PREFLABEL</p>
						<o></o>
					</matchPattern>
					<matchPattern type="EXACT">
						<s></s>
						<p>RDF_TYPE</p>
						<o>SKOS_CONCEPT</o>
					</matchPattern>
				</produces>
				
				<notice>currently produces: Category, i.e. not language independent</notice>
			</extractor>

			<extractor name ="org.dbpedia.extraction.mappings.RedirectExtractor" status="PURGE">
				<produces>
					<matchPattern type="EXACT">
						<s></s>
						<p>DB_REDIRECT</p>
						<o></o>
					</matchPattern>
				</produces>

				<notice>currently produces: Category, i.e. not language independent</notice>
			</extractor>
			
			
			<!-- The following extractors should be considered -->
			<extractor name ="org.dbpedia.extraction.mappings.CategoryLabelExtractor" status="PURGE">
				<produces>
					<matchPattern type="STARTSWITH">
						<s>DB_CATEGORY_NS</s>
						<p>RDFS_LABEL</p>
						<o></o>
					</matchPattern>
				</produces>
				
			</extractor>

			<extractor name ="org.dbpedia.extraction.mappings.ArticleCategoriesExtractor" status="PURGE">
				<produces>
					<matchPattern type="STARTSWITH">
						<s></s>
						<p>SKOS_SUBJECT</p>
						<o>DB_CATEGORY_NS</o>
					</matchPattern>
				</produces>

			</extractor>

			<extractor name ="org.dbpedia.extraction.mappings.MappingExtractor" status="ACTIVE">
				<produces>
					<matchPattern type="STARTSWITH">
						<s></s>
						<p>DB_ONTOLOGY_NS</p>
						<o></o>
					</matchPattern>
					<matchPattern type="EXACT">
						<s></s>
						<p>RDF_TYPE</p>
						<o>OWL_THING</o>
					</matchPattern>
					<matchPattern type="STARTSWITH" pexact="true">
						<s></s>
						<p>RDF_TYPE</p>
						<o>DB_COMMUNITY_NS</o>
					</matchPattern>
					<matchPattern type="STARTSWITHS">
						<s></s>
						<p>RDF_TYPE</p>
						<o>DB_COMMUNITY_NS</o>
					</matchPattern>
					<matchPattern type="STARTSWITHS">
						<s></s>
						<p>RDF_TYPE</p>
						<o>GML_NAMESPACE</o>
					</matchPattern>
					<matchPattern type="EXACT">
						<s></s>
						<p>FOAF_NAME</p>
						<o></o>
					</matchPattern>
					<matchPattern type="EXACT">
						<s></s>
						<p>FOAF_HOMEPAGE</p>
						<o></o>
					</matchPattern>
					<matchPattern type="EXACT">
						<s></s>
						<p>GEORSS_POINT</p>
						<o></o>
					</matchPattern>
					<matchPattern type="EXACT">
						<s></s>
						<p>GEORSS_RADIUS</p>
						<o></o>
					</matchPattern>
					<matchPattern type="EXACT">
						<s></s>
						<p>WGS_LAT</p>
						<o></o>
					</matchPattern>
					<matchPattern type="EXACT">
						<s></s>
						<p>WGS_LONG</p>
						<o></o>
					</matchPattern>
				</produces>
				<notice>Not sure whether those match patterns are totally correct or not</notice>
			</extractor>

			<extractor name ="org.dbpedia.extraction.mappings.PageIdExtractor" status="PURGE">
				<produces>
					<matchPattern type="EXACT">
						<s></s>
						<p>PAGE_ID</p>
						<o></o>
					</matchPattern>
				</produces>
			</extractor>

			<extractor name ="org.dbpedia.extraction.mappings.RevisionIdExtractor" status="PURGE">
				<produces>
					<matchPattern type="EXACT">
						<s></s>
						<p>REVISION_ID</p>
						<o></o>
					</matchPattern>
				</produces>
			</extractor>
		</extractors>
	</language>
		
    
</configuration>